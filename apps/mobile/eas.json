{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "http://localhost:3030/api",
        "WEB_BASE_URL": "http://localhost:8081",
        "CUSTOM_URL_SCHEME": "myapp-dev://",
        "APP_NAME": "MyApp",
        "APP_SLUG": "my-app",
        "IOS_BUNDLE_ID": "com.example.myapp",
        "ANDROID_PACKAGE_NAME": "com.example.myapp"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL": "https://staging-api.example.com/api",
        "WEB_BASE_URL": "https://staging-app.example.com",
        "CUSTOM_URL_SCHEME": "myapp-staging://",
        "APP_NAME": "MyApp",
        "APP_SLUG": "my-app",
        "IOS_BUNDLE_ID": "com.example.myapp",
        "ANDROID_PACKAGE_NAME": "com.example.myapp"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASE_URL": "https://api.example.com/api",
        "WEB_BASE_URL": "https://app.example.com",
        "CUSTOM_URL_SCHEME": "myapp://",
        "APP_NAME": "MyApp",
        "APP_SLUG": "my-app",
        "IOS_BUNDLE_ID": "com.example.myapp",
        "ANDROID_PACKAGE_NAME": "com.example.myapp"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
